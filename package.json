{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "demo api basic",
  "main": "server.ts",
  "scripts": {
    "start": "node -r ts-node/register src/server.ts",
    "start-dev": "node -r ts-node/register src/server.ts",
    "build": "tsc -p .",
    "prod-copy": "copyfiles -u 1 src/**/*.js build/src/",
    "dev": "nodemon",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "posttest": "npm run lint"
  },
  "keywords": [
    "rest-api"
  ],
  "author": "tuannv191292@gmail.com",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "index.ts"
    ],
    "exec": "npm run start-dev",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jws": "^3.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.22",
    "@types/nodemailer": "^6.4.5",
    "@types/stripe": "^8.0.417",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "gts": "^4.0.0",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "tsoa": "^4.1.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "app-root-path": "^3.0.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "ioredis": "^5.2.2",
    "jws": "^4.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "rate-limit-redis": "^3.0.1",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.3",
    "socket.io": "^4.5.1",
    "stripe": "^10.5.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0"
  }
}
